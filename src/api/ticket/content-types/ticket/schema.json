{
  "kind": "collectionType",
  "collectionName": "tickets",
  "info": {
    "singularName": "ticket",
    "pluralName": "tickets",
    "displayName": "ticket",
    "description": "Ticket products for events"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "minimal_order": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "minimal_order_date": {
      "type": "date"
    },
    "variant": {
      "type": "component",
      "repeatable": true,
      "component": "variant-product.variant-product"
    },
    "main_image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "default"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "terms_conditions": {
      "type": "customField",
      "options": {
        "preset": "default"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "rate": {
      "type": "decimal",
      "default": 5
    },
    "kabupaten": {
      "type": "string",
      "required": false
    },
    "region": {
      "type": "string"
    },
    "sold_count": {
      "type": "integer",
      "default": 0
    },
    "escrow": {
      "type": "boolean",
      "default": false
    },
    "blogs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog.blog",
      "mappedBy": "tickets"
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "pending",
        "rejected",
        "approved"
      ],
      "default": "pending"
    },
    "event_date": {
      "type": "date",
      "required": false
    },
    "waktu_event": {
      "type": "string"
    },
    "lokasi_event": {
      "type": "string"
    },
    "kota_event": {
      "type": "string"
    },
    "end_date": {
      "type": "date"
    },
    "end_time": {
      "type": "string"
    }
  }
}
